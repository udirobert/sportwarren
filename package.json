    "@supabase/supabase-js": "^2.39.3",
    "@xmtp/react-sdk": "^7.0.0",
    "@xmtp/xmtp-js": "^11.3.0",
    "algosdk": "^3.3.1",
    "apollo-server-express": "^3.12.1",
    "aws-sdk": "^2.1550.0",
